{
  "name": "armory-react",
  "version": "4.28.1",
  "private": true,
  "pre-commit": [
    "test"
  ],
  "scripts": {
    "start": "node ./scripts/start.js --production",
    "start:embeds": "LOCAL=true npm start -- --embeds --local",
    "test": "npm run lint && npm run flow && npm run coverage",
    "flow": "flow",
    "flowtypes": "flow-typed install --overwrite",
    "lint": "eslint ./config ./scripts ./src ./test",
    "dev": "node ./scripts/start.js",
    "dev:embeds": "npm run dev -- --embeds",
    "build": "node ./scripts/build.js",
    "deploy": "node ./scripts/deploy.js",
    "unit": "BABEL_ENV=test mocha \"src/**/*.spec.js\"",
    "tdd": "npm run unit -- -w",
    "coverage": "BABEL_ENV=test nyc npm run unit",
    "codecov": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "preversion": "npm test",
    "version": "version-changelog CHANGELOG.md && changelog-verify CHANGELOG.md && git add CHANGELOG.md",
    "postversion": "git push && git push --tags",
    "generateColourMap": "rm src/assets/categoryColourMap.json && node scripts/extractAchievementCategoriesColour.js >> src/assets/categoryColourMap.json"
  },
  "dependencies": {
    "axios": "^0.16.1",
    "babel-polyfill": "^6.23.0",
    "classnames": "^2.2.5",
    "fastclick": "^1.0.6",
