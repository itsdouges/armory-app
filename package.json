    "gw2itemstats": "^0.0.2",
    "i18n-react": "^0.3.0-ts1",
    "lodash": "^4.14.1",
    "lz-string": "^1.4.4",
    "moment": "^2.15.2",
    "normalize.css": "^7.0.0",
    "react": "^15.6.1",
    "react-debounce-decorator": "^0.1.3",
    "react-dom": "^15.6.1",
    "react-helmet": "^5.1.3",
    "react-redux": "^5.0.6",
    "react-responsive": "^1.3.4",
    "react-router-dom": "^4.2.0",
    "react-scroll-paginator": "^0.1.2",
    "react-sticky-header": "^0.1.9",
    "recompose": "^0.25.0",
    "redux": "^3.7.2",
    "redux-thunk": "^2.1.0",
    "reselect": "^3.0.1"
  },
  "devDependencies": {
