    "gw2itemstats": "^0.0.2",
    "i18n-react": "^0.3.0-ts1",
    "lodash": "^4.14.1",
    "lz-string": "^1.4.4",
    "moment": "^2.15.2",
    "normalize.css": "^7.0.0",
    "react": "^15.3.0",
    "react-debounce-decorator": "^0.1.3",
    "react-dom": "^15.4.2",
    "react-helmet": "^5.1.3",
    "react-redux": "^5.0.5",
    "react-responsive": "^1.3.0",
    "react-router-dom": "^4.1.1",
    "react-scroll-paginator": "^0.1.2",
    "react-sticky-header": "^0.1.6",
    "recompose": "^0.23.4",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0",
    "reselect": "^3.0.1",
    "withinviewport": "^2.0.0"
  },
  "devDependencies": {
    "app-module-path": "^2.2.0",
